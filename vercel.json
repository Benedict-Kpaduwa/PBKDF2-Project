{
  "headers": [
    {
      "source": "/(.*\\.(?:mp3|wav))",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" },
        { "key": "Content-Type", "value": "audio/wav" }
      ]
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ]
}
